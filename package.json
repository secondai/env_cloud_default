{
  "name": "express-babel",
  "version": "1.0.0",
  "description": "Using latest JavaScript features on the server",
  "repository": "https://github.com/vmasto/express-babel",
  "author": "Vassilis Mastorostergios <vmasto@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "engines": {
    "node": "~8.9.0",
    "npm": ">=3.10.0"
  },
  "scripts": {
    "prestart": "npm run -s build",
    "server:start": "node dist/index.js",
    "server:dev": "nodemon src/index.js --exec \"node -r dotenv/config -r babel-register\"",
    "server:dev-inspect": "nodemon src/index.js --exec \"node --inspect -r dotenv/config -r babel-register\"",
    "server:heroku": "node_modules/nodemon/bin/nodemon.js src/index.js --exec \"node -r dotenv/config -r babel-register\"",
    "server:clean": "rimraf dist",
    "server:build": "npm run server:clean && mkdir -p dist && babel src -s -D -d dist",
    "server:test": "jest --watch",
    "server:lint": "esw -w src test"
  },
  "keywords": [
    "express",
    "babel",
    "boilerplate",
    "scaffold",
    "es6",
    "es2015",
    "es2016",
    "es2017",
    "jest",
    "eslint"
  ],
  "dependencies": {
    "@stdlib/stdlib": "^0.0.30",
    "alasql": "^0.4.3",
    "apollo-server-express": "^1.2.0",
    "aws-sdk": "^2.167.0",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^21.0.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.4.0",
    "babel-register": "^6.24.1",
    "bigi": "^1.4.2",
    "bitcoinjs-lib": "^3.3.1",
    "body-parser": "^1.17.1",
    "compression": "^1.7.1",
    "concurrent-queue": "^7.0.2",
    "connect-mongo": "^2.0.0",
    "connect-redis": "^3.3.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "crypto-js": "^3.1.9-1",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "express-session": "^1.15.6",
    "getstream": "^3.9.0",
    "graphql": "^0.11.7",
    "graphql-client": "^2.0.0",
    "graphql-compose": "^2.9.6",
    "graphql-compose-dataloader": "^1.1.2",
    "graphql-compose-mongoose": "^1.9.4",
    "graphql-subscriptions": "^0.5.4",
    "graphql-tag": "^2.5.0",
    "handlebars": "^4.0.11",
    "hashcash": "^0.5.1",
    "helmet": "^3.9.0",
    "js-schema": "^1.0.1",
    "jszip": "3.1.5",
    "jszip-utils": "0.0.2",
    "luxon": "^0.0.19",
    "minimist": "^1.2.0",
    "mongoose": "^4.13.0",
    "morgan": "^1.9.0",
    "nedb": "^1.8.0",
    "nedb-promise": "^2.0.1",
    "newrelic": "^2.4.1",
    "node-ipc": "^9.1.1",
    "node-rsa": "^0.4.2",
    "nodemon": "^1.12.5",
    "parse-github-url": "1.0.2",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pug": "^2.0.0-beta11",
    "queue": "^4.4.2",
    "redis": "^2.8.0",
    "request": "^2.83.0",
    "request-promise-native": "1.0.5",
    "rimraf": "^2.6.1",
    "safe-eval": "^0.3.0",
    "string-similarity": "^1.2.0",
    "subscriptions-transport-ws": "^0.9.1",
    "uuid": "^3.1.0",
    "vm2": "^3.5.2",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "eslint": "^4.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jest": "^21.0.2",
    "eslint-watch": "^3.1.0",
    "jest": "^21.0.2",
    "supertest": "^3.0.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "import",
      "jest"
    ],
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "env": {
      "node": true,
      "jest": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "rules": {
      "jest/no-focused-tests": 2,
      "jest/no-identical-title": 2
    }
  },
  "jest": {
    "testEnvironment": "node"
  }
}
